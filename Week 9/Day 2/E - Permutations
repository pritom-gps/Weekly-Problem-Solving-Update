#include <bits/stdc++.h>
#define ll long long
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    int n;
    cin >> n;
    if (n >= 2 && n < 4)
        cout << "NO SOLUTION" << '\n';
    else
    {
        for (int i = n; i >= 1; i--)
        {
            if (i % 2 != 0)
                cout << i << " ";
        }
        for (int i = n; i >= 1; i--)
        {
            if (i % 2 == 0)
                cout << i << " ";
        }
    }
    return 0;
}
